<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>officeweb</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>helloWorld</servlet-name>
		<servlet-class>com.training.officeweb.HelloWorld</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>helloWorld</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>HelloWorldPrashanth</display-name>
		<servlet-name>HelloWorldPrashanth</servlet-name>
		<servlet-class>com.training.prashanth.HelloWorldPrashanth</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloWorldPrashanth</servlet-name>
		<url-pattern>/HelloWorldPrashanth</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LoginServlet</display-name>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.training.officeweb.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<display-name>LoginServletPradeep</display-name>
		<servlet-name>LoginServletPradeep</servlet-name>
		<servlet-class>com.training.officeweb.Login1Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServletPradeep</servlet-name>
		<url-pattern>/pradeep/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LogonServlet</display-name>
		<servlet-name>LogonServlet</servlet-name>
		<servlet-class>com.training.officeweb.LogonServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogonServlet</servlet-name>
		<url-pattern>/Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>Register</display-name>
		<servlet-name>Register</servlet-name>
		<servlet-class>com.training.prashanth.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ServletDemo</display-name>
		<servlet-name>ServletDemo</servlet-name>
		<servlet-class>com.training.ServletDemo</servlet-class>
		<init-param>
			<param-name>name</param-name>
			<param-value>Hari</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletDemo</servlet-name>
		<url-pattern>/servlet</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>name</param-name>
		<param-value>Hari</param-value>
	</context-param>
	
	  <servlet>
    <description></description>
    <display-name>RedirectForwardDemo</display-name>
    <servlet-name>RedirectForwardDemo</servlet-name>
    <servlet-class>com.training.RedirectForwardDemo</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedirectForwardDemo</servlet-name>
    <url-pattern>/redirectForward</url-pattern>
  </servlet-mapping>

</web-app>